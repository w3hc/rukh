{
  "name": "rukh",
  "version": "0.2.0",
  "description": "Nest.js-based AI agent starter kit",
  "author": "W3HC",
  "license": "LGPL-3.0-or-later",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.ts && jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:cov": "jest --config ./test/jest-e2e.json && jest --coverage --config jest.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.ts",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:concurrent": "jest test/concurrent-requests.e2e-spec.ts --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@langchain/core": "^1.1.8",
    "@langchain/mistralai": "^1.0.2",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "ethers": "^6.16.0",
    "multer": "2.0.2",
    "puppeteer-core": "^24.34.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@jest/types": "^30.2.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "elliptic": ">=6.6.0",
      "ws": ">=8.17.1",
      "axios": ">=1.13.2",
      "pbkdf2": ">=3.1.5",
      "sha.js": ">=2.4.11",
      "form-data": ">=4.0.1",
      "base-x": ">=3.0.11",
      "undici": ">=5.29.0",
      "js-yaml": ">=4.1.0",
      "brace-expansion": ">=2.0.2",
      "tmp": ">=0.2.3"
    }
  }
}